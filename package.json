    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "transformIgnorePatterns": [],
    "reporters": [
      "default"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "19.2.17",
    "@angular/common": "19.2.14",
