{
  "name": "phased",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run phased:storybook",
    "build-storybook": "ng run phased:build-storybook",
    "chromatic": "npx chromatic --project-token=137f9b2a9d87",
    "playwright": "npx playwright test",
    "size": "ng build --stats-json",
    "display": "npx esbuild-visualizer --metadata=dist/phased/stats.json --open=true"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "transformIgnorePatterns": [],
    "reporters": [
      "default"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.0.1",
    "@angular/cdk": "20.0.2",
    "@angular/common": "20.0.1",
    "@angular/compiler": "20.0.1",
    "@angular/core": "^20.0.1",
    "@angular/forms": "20.0.1",
    "@angular/material": "20.0.2",
    "@angular/platform-browser": "20.0.1",
    "@angular/platform-browser-dynamic": "20.0.1",
    "@angular/router": "20.0.1",
    "@babylonjs/core": "^8.8.3",
    "@babylonjs/inspector": "8.8.3",
    "@ngrx/signals": "^19.1.0",
    "@storybook/test": "^8.6.14",
    "echarts": "^5.6.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.2000.1",
    "@angular-devkit/build-angular": "20.0.1",
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/cli": "20.0.1",
    "@angular/compiler-cli": "20.0.1",
    "@babel/core": "^7.27.3",
    "@chromatic-com/storybook": "^3.2.6",
    "@compodoc/compodoc": "^1.1.26",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/angular": "^8.6.14",
    "@storybook/manager-api": "^8.6.2",
    "@storybook/theming": "^8.6.14",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "chromatic": "^12.0.0",
    "eslint": "^9.24.0",
    "eslint-plugin-storybook": "^0.12.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-preset-angular": "^14.5.5",
    "ng-mocks": "^14.13.4",
    "storybook": "^8.6.14",
    "typescript": "^5.8.3"
  }
